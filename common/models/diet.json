{
  "name": "Diet",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "description": {
      "type": "string",
      "required": true
    },
    "totalCarbohydrates": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4
    },
    "totalProteins": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4
    },
    "totalFats": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4
    },
    "totalCalories": {
      "type": "number",
      "required": true,
      "dataType": "decimal",
      "precision": 10,
      "scale": 4
    }
  },
  "validations": [],
  "relations": {
    "dietFoodDetails": {
      "type": "hasMany",
      "model": "Diet_Food_Detail",
      "foreignKey": "dietId"
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": ""
    }
  },
  "mixins": {
    "TimeStamp" : {
      "createdAt" : "createdAt",
      "updatedAt" : "updatedAt",
      "required" : false,
      "validateUpsert": true
    },
    "ReadOnly" : {
      "createdAt" : true,
      "updatedAt" : true
    }
  },
  "acls": [

    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },


    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "regular",
      "permission": "ALLOW",
      "property": "create"
    },

    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "premium",
      "permission": "ALLOW",
      "property": "create"
    },

    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },

  ],
  "methods": {}
}
