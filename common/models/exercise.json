{
  "name": "Exercise",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "imageName": {
      "type": "string",
      "required": true
    }
  },
  "mixins": {
    "TimeStamp" : {
      "createdAt" : "createdAt",
      "updatedAt" : "updatedAt",
      "required" : false,
      "validateUpsert": true
    },
    "ReadOnly" : {
      "createdAt" : true,
      "updatedAt" : true
    }
  },
  "validations": [],
  "relations": {
    "exerciseRoutineDetails": {
      "type": "hasMany",
      "model": "Exercise_Routine_Detail",
      "foreignKey": "exerciseId"
    },
    "bodyAreaExerciseDetails": {
      "type": "hasMany",
      "model": "BodyArea_Exercise_Detail",
      "foreignKey": "exerciseId"
    }
  },
  "acls": [],
  "methods": {}
}
